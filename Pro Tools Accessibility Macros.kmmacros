sys = Application("System Events")
pt = sys.applicationProcesses.byName("Pro Tools")
windows = pt.windows
mix = windows.whose({name:{_beginsWith:"Mix:"}})[0]
tracks = mix.groups
for (t=0; t&lt;tracks.length; t++) {
track = tracks[t]
name = track.name()
if (name.indexOf("Group List")&gt;-1|name.indexOf("master")&gt;-1|name.indexOf("aux")&gt;-1|name.indexOf("Aux")&gt;-1) continue
arm = track.buttons.byName("Track Record Enable")
if (arm.value().indexOf("armed")&gt;-1) {
arm.click()
}
}
vo.output ("Disarmed all tracks.")
