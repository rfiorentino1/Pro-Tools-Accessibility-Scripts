sys = Application("System Events")
pt = sys.applicationProcesses.byName("Pro Tools")
windows = pt.windows()
closed = 0
msg = ""
for (i=0; i&lt;windows.length; i++) {
window = windows[i];
name = windows[i].name()
if (name.indexOf("Mix")==-1 &amp; name.indexOf("Edit")==-1) {
msg = msg+"closing "+name+". "
closed = closed+1
 window.buttons[0].click()
}
}
if (closed==0) msg = msg+"No windows to cloz."
vo.output (msg)
